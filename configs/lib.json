{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ESNext"],
    "declaration": true,
    "declarationMap": true,
    "moduleResolution": "Node",
    "moduleDetection": "force",
    "resolveJsonModule": true,
    "sourceMap": true,
    "isolatedModules": true
  }
}
