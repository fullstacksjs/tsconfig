{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "moduleResolution": "Bundler",
    "allowJs": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "noEmit": true,
    "resolveJsonModule": true,
    "plugins": [{ "name": "next" }]
  }
}
